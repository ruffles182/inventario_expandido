<odoo>
  <data>
  <!-- views clave sat -->
      <record id="view_clave_sat_tree" model="ir.ui.view">
          <field name="name">view.inentario.expandido.clave_sat.tree</field>
          <field name="model">inventario_expandido.clave_sat</field>
          <field name="arch" type="xml">
          <tree string="Claves SAT">
              <field name="clave"/>
          </tree>
          </field>
      </record>

      <record id="view_clave_sat_form" model="ir.ui.view">
          <field name="name">view.inentario.expandido.clave_sat.form</field>
          <field name="model">inventario_expandido.clave_sat</field>
          <field name="arch" type="xml">
          <form name="clave_sat_form">
              <sheet>
              <div class="oe_title">
                  <group>
                      <field name="clave"/>
                  </group>
              </div>
              </sheet>
          </form>
          </field>
      </record>

      <record id="action_clave_sat_views" model="ir.actions.act_window">
          <field name="name">Clave sat</field>
          <field name="type">ir.actions.act_window</field>
          <field name="res_model">inventario_expandido.clave_sat</field>
          <field name="view_type">form</field>
          <field name="view_mode">tree,form</field>
          <field name="help" type="html">
          <p class="oe_view_nocontent_create">
              Claves SAT
          </p>
          <p>
              Agregue una clave SAT
          </p>
          </field>
      </record>
      <menuitem id="menu_clave_sat" name="Claves SAT" parent="stock.menu_stock_inventory_control" action="action_clave_sat_views" sequence="10"/>

      <!-- views Unidad de medida SAT -->
      <record id="view_medida_sat_tree" model="ir.ui.view">
          <field name="name">view.inentario.expandido.medida_sat.tree</field>
          <field name="model">inventario_expandido.medida_sat</field>
          <field name="arch" type="xml">
          <tree string="Medidass SAT">
              <field name="medida"/>
          </tree>
          </field>
      </record>

      <record id="view_medida_sat_form" model="ir.ui.view">
          <field name="name">view.inentario.expandido.medida_sat.form</field>
          <field name="model">inventario_expandido.medida_sat</field>
          <field name="arch" type="xml">
          <form name="medida_sat_form">
              <sheet>
              <div class="oe_title">
                  <group>
                      <field name="medida"/>
                  </group>
              </div>
              </sheet>
          </form>
          </field>
      </record>

      <record id="action_medida_sat_views" model="ir.actions.act_window">
          <field name="name">medida sat</field>
          <field name="type">ir.actions.act_window</field>
          <field name="res_model">inventario_expandido.medida_sat</field>
          <field name="view_type">form</field>
          <field name="view_mode">tree,form</field>
          <field name="help" type="html">
          <p class="oe_view_nocontent_create">
              mUnidades de medidas SAT
          </p>
          <p>
              Agregue una Unidad de medida SAT
          </p>
          </field>
      </record>
      <menuitem id="menu_medida_sat" name="Unidade medidas SAT" parent="stock.menu_stock_inventory_control" action="action_medida_sat_views" sequence="11"/>

    <!-- vista heredada producto -->
    <record id="view_template_propiedadeds_form" model="ir.ui.view">
        <field name="name">product.template.inventatro_expandido.propiedades.form.inherit</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='group_general']" position="before">
              <group>
                <field name="ubicacion" />
                <field name="pedimento" />
                <field name="clave_sat" />
                <field name="unidad_sat" />
              </group>
            </xpath>
        </field>
    </record>

    <!-- vista heredada stock picking -->
    <record id="view_picking_form_extend" model="ir.ui.view">
        <field name="name">stock.picking.form.extend</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <field name="origin" position="after">
                <field name="empleado"/>
            </field>
        </field>
    </record>
  </data>
</odoo>